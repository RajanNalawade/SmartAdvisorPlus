<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="app_theme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowBackground">@drawable/app_background</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#2B3990</item>
        <item name="android:windowTitleSize">50dp</item>
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="tv_heading" parent="@android:style/Widget.TextView">
        <item name="android:gravity">center_vertical</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:textColor">@color/White</item>
        <item name="android:textSize">@dimen/ts_heading_menu</item>
        <item name="android:paddingLeft">@dimen/default_padding</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">@dimen/heading_height</item>
    </style>

    <style name="btnStyleBreakerBay" parent="@android:style/Widget.Button">
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:gravity">center</item>
        <item name="android:shadowColor">#000000</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">0.6</item>
        <item name="android:background">@drawable/custom_btn_breaker_bay</item>
        <item name="android:padding">10dip</item>
    </style>

    <style name="btnStyleGenoa" parent="@android:style/Widget.Button">
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:gravity">center</item>
        <item name="android:shadowColor">#000000</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">0.6</item>
        <item name="android:background">@drawable/btn_theme</item>
        <item name="android:padding">10dip</item>
    </style>

    <style name="cb_default">
        <item name="android:textColor">@color/Black</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="tr_header_text">
        <item name="android:textColor">@color/White</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="et_default">
        <item name="android:background">@drawable/rounder_corner_shape</item>
        <item name="android:textSize">@dimen/ts_default</item>
        <item name="android:singleLine">true</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/Black</item>
        <item name="android:layout_height">@dimen/default_height</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>

    <style name="tv_tableRow" parent="@android:style/Widget.TextView">
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/Black</item>
        <item name="android:textSize">@dimen/ts_default</item>
        <item name="android:paddingLeft">@dimen/default_padding</item>
        <item name="android:layout_height">@dimen/form_row_height</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="tv_default" parent="@android:style/Widget.TextView">
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/Black</item>
        <item name="android:textSize">@dimen/ts_default</item>
        <item name="android:paddingLeft">@dimen/default_padding</item>
        <item name="android:minHeight">@dimen/default_height</item>
    </style>

    <style name="btn_default">
        <item name="android:layout_height">@dimen/btn_height</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:background">@drawable/focus_button</item>
        <item name="android:textColor">@color/White</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/ts_btn</item>
    </style>

    <style name="tr_default">
        <item name="android:layout_height">@dimen/form_row_height</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:padding">05dp</item>
    </style>

    <style name="SeekBarAppTheme" parent="android:Widget.Holo.Light.SeekBar">
        <item name="android:progressDrawable">
            @drawable/apptheme_scrubber_progress_horizontal_holo_light
        </item>
        <item name="android:indeterminateDrawable">
            @drawable/apptheme_scrubber_progress_horizontal_holo_light
        </item>
        <item name="android:thumb">@drawable/apptheme_scrubber_control_selector_holo_light</item>
    </style>

    <style name="spnr_default">
        <item name="android:background">@drawable/focus_spinner</item>
        <item name="android:popupBackground">@drawable/border_spinner</item>
        <item name="android:textSize">@dimen/ts_default</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/Black</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_height">@dimen/spnr_height</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:spinnerMode">dropdown</item>
    </style>

    <style name="app_test" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowBackground">@drawable/app_back</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#2B3990</item>
        <item name="android:windowTitleSize">50dp</item>
        <item name="android:windowFullscreen">true</item>
    </style>

    <!--  <style name="app_splash" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowBackground">@drawable/app_backsplash</item>
    </style> -->

    <style name="CustomTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowBackground">@drawable/app_back</item>
        <item name="android:textSize">20sp</item>
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="MyTabStyle" parent="@android:style/TextAppearance.Widget.TabWidget">
        <item name="android:textColor">#91BDE5</item>
    </style>

    <style name="LightTabWidget" parent="@android:style/Widget.TabWidget">
        <item name="android:textColor">@color/tab_indicator_text</item>
    </style>

    <style name="app_themetitle" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowBackground">@drawable/app_background</item>
        <item name="android:windowTitleBackgroundStyle">@drawable/app_background</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/Black</item>
        <item name="android:windowTitleSize">45dp</item>
        <item name="android:windowFullscreen">true</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="app_testtitle" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowBackground">@drawable/app_back</item>
        <item name="android:windowTitleBackgroundStyle">@drawable/app_background</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#2B3990</item>
        <item name="android:windowTitleSize">45dp</item>
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="app_common" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowBackground">@drawable/app_back</item>
        <item name="android:windowTitleBackgroundStyle">@drawable/app_background</item>
        <item name="android:textSize">15sp</item>
        <!--         <item name="android:textStyle">bold</item> -->
        <item name="android:textColor">#000000</item>
        <item name="android:windowTitleSize">45dp</item>
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="MooTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:spinnerItemStyle">@style/SpinnerItem</item>
        <item name="android:spinnerDropDownItemStyle">@style/SpinnerItem.DropDownItem</item>
    </style>

    <style name="SpinnerItem" parent="@android:style/Widget.TextView.SpinnerItem">
        <item name="android:textColor">#000000</item>
    </style>

    <style name="SpinnerItem.DropDownItem" parent="@android:style/Widget.DropDownItem.Spinner">
        <item name="android:textColor">#000000</item>
    </style>

    <style name="NewButton.Button" parent="@android:style/Widget.Button">
        <item name="android:background">@drawable/logout</item>
    </style>

    <style name="app_TextColor">
        <item name="android:textColor">#000000</item>
    </style>

    <style name="editetxt_style">
        <item name="android:background">@drawable/shape</item>
    </style>

    <style name="button_style">
        <item name="android:background">@drawable/btn_indicator</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>

    <style name="spinner_style" parent="@android:style/Widget.TextView.SpinnerItem">
        <item name="android:background">@drawable/spinner_background</item>
    </style>

    <style name="datepickerstyle" parent="@android:style/Theme.Holo.Light">
        <item name="android:startYear">1900</item>
        <item name="android:endYear">2200</item>
    </style>

    <style name="AppBaseThemeActionbar" parent="Theme.AppCompat.Light">
        <item name="android:textColor">#000000</item>
    </style>

    <style name="btnStyleSeagull" parent="@android:style/Widget.Button">
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#626954</item>
        <item name="android:gravity">center</item>
        <item name="android:shadowColor">#000000</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">0.6</item>
        <item name="android:background">@drawable/cifenrollment_custom_btn_seagull</item>
        <item name="android:padding">10dip</item>
    </style>

    <style name="MyAlertDialogStyle" parent="Theme.AppCompat.Light.Dialog.Alert">
        <!-- Used for the buttons -->
        <item name="colorAccent">@color/tabDark</item>
        <!-- Used for the title and text -->
        <item name="android:textColorPrimary">#000000</item>
        <!-- Used for the background -->
        <item name="android:background">#FFFFFF</item>
    </style>

    <style name="MyAlertDialogStyle1" parent="ThemeOverlay.AppCompat">
        <!-- Used for the buttons -->
        <item name="colorAccent">@color/tabDark</item>
        <!-- Used for the title and text -->
        <item name="android:textColorPrimary">#ffffff</item>
        <!-- Used for the background -->
        <item name="android:background">@android:color/transparent</item>

    </style>

    <style name="spinnerItemStyle" parent="android:Widget.TextView.SpinnerItem"></style>

    <style name="spinnerDropDownItemStyle" parent="android:TextAppearance.Widget.TextView.SpinnerItem"></style>

    <style name="app_background">
        <item name="android:background">#FFFFFF</item>
    </style>

    <style name="AlertDialogCustom" parent="@android:style/Theme.Dialog">
        <item name="android:textColor">#00FF00</item>
        <item name="android:typeface">monospace</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="Widget.app_themetitle.ActionBar.Fullscreen" parent="Widget.AppCompat.ActionBar">
        <item name="android:background">@color/black_overlay</item>
    </style>

    <style name="Widget.app_themetitle.ButtonBar.Fullscreen" parent="">
        <item name="android:background">@color/black_overlay</item>
        <item name="android:buttonBarStyle">?android:attr/buttonBarStyle</item>
    </style>

</resources>