<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    android:overScrollMode="always" 
    >
<LinearLayout
        android:id="@+id/LinearLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" 
        android:background="@drawable/header" >

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:text="SBI LIFE - SCHOLAR II"
            android:textAppearance="?android:attr/textAppearanceSmall" 
            android:textColor="@android:color/white" />
        
         <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:text="(UIN:111N020V01)"
            android:textAppearance="?android:attr/textAppearanceSmall" 
            android:textColor="@android:color/white" />
    
    </LinearLayout>
<ScrollView
    android:id="@+id/ScrollView01"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content" >

<TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:paddingTop="6dp"
         android:paddingBottom="6dp"
         >

<TableRow
            android:id="@+id/tableRow1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"            
         >


    <CheckBox
        android:id="@+id/cb_staffdisc"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:text="Staff Discount"
        android:layout_weight="1" 
         style="@style/app_TextColor"
         android:visibility="gone"/>
</TableRow>
	<TableRow
            android:id="@+id/tableRow2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"            
         >
    

                 <LinearLayout 
	    android:id="@+id/linerarlayout8"
	    android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="0.5"
        android:orientation="vertical">
     <TextView
        android:id="@+id/textview8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Age:"
        android:textAppearance="?android:attr/textAppearanceMedium"
         style="@style/app_TextColor"
         />
		
		<TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="(As on Last Birthday)"
        android:textColor="#4C4C4C"
        android:textAppearance="?android:attr/textAppearanceMedium"
         />
        </LinearLayout>

        <Spinner
            android:id="@+id/age"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.5"
             android:background="@drawable/bgspinner"
                android:popupBackground="@drawable/myspinner_border"
                android:spinnerMode="dropdown" />
   </TableRow>
   <TableRow
            android:id="@+id/tableRow3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"            
         >


   <LinearLayout 
	    android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="0.5"
        android:orientation="vertical">
     <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Child Age:"
        android:textAppearance="?android:attr/textAppearanceMedium"
         style="@style/app_TextColor"
         />
		
		<TextView

        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="(As on Last Birthday)"
        android:textColor="#4C4C4C"
        android:textAppearance="?android:attr/textAppearanceMedium"
         />
        </LinearLayout>
        
        
        <Spinner
            android:id="@+id/childage"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.5"
             android:background="@drawable/bgspinner"
                android:popupBackground="@drawable/myspinner_border"
                android:spinnerMode="dropdown" />
    </TableRow>
    
 <TableRow
            android:id="@+id/tableRow4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"   
            android:layout_marginTop="5dp"         
         >


        <TextView
             style="@style/app_TextColor"
            android:id="@+id/textView5"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="Gender:"
            android:textAppearance="?android:attr/textAppearanceMedium" 
            android:layout_weight="0.5"/>

        <Spinner
            android:id="@+id/selGender"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.5"
             android:background="@drawable/bgspinner"
                android:popupBackground="@drawable/myspinner_border"
                android:spinnerMode="dropdown" />
   </TableRow>
<TableRow
            android:id="@+id/tableRow5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
              android:layout_marginTop="5dp"               
         >


        <TextView
             style="@style/app_TextColor"
            android:id="@+id/textView2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.5"
            android:text="Policy Term :"
            android:textAppearance="?android:attr/textAppearanceMedium" />

        <EditText
             style="@style/editetxt_style"
            android:id="@+id/policyterm"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="number" 
            android:layout_weight="0.5"/>
   </TableRow>
    
   <TableRow
            android:id="@+id/tableRow6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"     
              android:layout_marginTop="5dp"          
         >

        <TextView
             style="@style/app_TextColor"
            android:layout_width="0dp"
            android:layout_height="54dp"
            android:text="Premium paying Term :"
            android:textAppearance="?android:attr/textAppearanceMedium" 
            android:layout_weight="0.5"/>

        <EditText
             style="@style/editetxt_style"
            android:id="@+id/premterm"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="number" 
            android:layout_weight="0.5">
        </EditText>
   </TableRow>
  <TableRow
            android:id="@+id/tableRow7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"       
              android:layout_marginTop="5dp"        
         >

        <TextView
             style="@style/app_TextColor"
            android:id="@+id/textView3"
            android:layout_width="0dp"
            android:layout_height="56dp"
            android:text="Premium Payment Mode:"
            android:textAppearance="?android:attr/textAppearanceMedium" 
            android:layout_weight="0.5"/>

        <Spinner
            android:id="@+id/premiumfreq"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.5"
             android:background="@drawable/bgspinner"
                android:popupBackground="@drawable/myspinner_border"
                android:spinnerMode="dropdown" />
    </TableRow>
	 <TableRow
            android:id="@+id/tableRow8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
              android:layout_marginTop="5dp"               
         >

        <TextView
            android:id="@+id/prem_amt"
            android:layout_width="0dp"
            android:layout_height="55dp"
             style="@style/app_TextColor"
            android:text="Sum Assured(Rs.)"
            android:textAppearance="?android:attr/textAppearanceMedium" 
            android:layout_weight="0.5"/>

        <EditText
            android:id="@+id/premium_amt"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:ems="10"
             style="@style/editetxt_style"
            android:inputType="number" 
            android:layout_weight="0.5">
        </EditText>
    </TableRow>
<TableRow
            android:id="@+id/tableRow9"
