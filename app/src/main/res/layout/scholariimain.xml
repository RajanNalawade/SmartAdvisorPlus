    <TextView
        android:id="@+id/selCC13NonLinked_txt1"
        android:layout_width="0dp"
        android:layout_height="50dp"
         style="@style/app_TextColor"
        android:text="DS-Individual CI rider Term:"
        android:visibility="gone" 
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_weight="0.5"/>

    <Spinner
        android:id="@+id/selCC13NonLinkedTerm"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="number"
        android:visibility="gone" 
        android:layout_weight="0.5"
         android:background="@drawable/bgspinner"
                android:popupBackground="@drawable/myspinner_border"
                android:spinnerMode="dropdown" />
</TableRow>
<TableRow
            android:id="@+id/tableRow14"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" 
            android:visibility="gone"           
         >
    <TextView
        android:id="@+id/selCC13NonLinked_txt2"
        android:layout_width="0dp"
        android:layout_height="80dp"
         style="@style/app_TextColor"
        android:text="DS-Individual CI rider Sum Assured(Rs):"
       android:textAppearance="?android:attr/textAppearanceMedium"
        android:visibility="gone" 
        android:layout_weight="0.5"/>

    <EditText
        android:id="@+id/cc13NonLinkedSA"
        android:layout_width="0dp"
         style="@style/editetxt_style"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="number"
        android:visibility="gone" 
        android:layout_weight="0.5"/>
</TableRow>
<TableRow
            android:id="@+id/tableRow15"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"            
         >
    <CheckBox
         style="@style/app_TextColor"
        android:id="@+id/premwaiverRider"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:text="Premium Waiver Benefit Rider (UIN:111B005V01)"  
        android:layout_weight="0.5"/>
    </TableRow>
     <TableRow
            android:id="@+id/tableRow18"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"            
         >
    <Button
         android:id="@+id/btnSubmit"
         android:layout_width="0dp"
         android:layout_height="wrap_content"
         android:layout_weight="0.5"
         android:text="Calculate"
         style="@style/button_style"
          android:layout_margin="2dp" />
    


     <Button
         android:id="@+id/back"
         android:layout_width="0dp"
         android:layout_height="wrap_content"
         android:layout_weight="0.5"
         android:text="Cancel"
         style="@style/button_style"
          android:layout_margin="2dp" />

 </TableRow>
</TableLayout>

</ScrollView>

</LinearLayout>