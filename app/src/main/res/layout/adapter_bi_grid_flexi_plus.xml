<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">

	<TableRow
		android:id="@+id/tr_header2"
		android:layout_width="fill_parent"
		android:layout_height="fill_parent"
		android:weightSum="47">

		<TextView
			android:id="@+id/tv_policy_year"
			style="@style/tv_heading"
			android:layout_width="0dp"
			android:layout_height="fill_parent"
			android:layout_weight="1"
			android:text="Policy Year"
			android:textColor="@color/Black"
			android:textSize="14sp" />



		<TextView
			android:id="@+id/tv_premium"
			style="@style/tv_heading"
			android:layout_width="0dp"
			android:layout_height="fill_parent"
			android:layout_weight="1"
			android:text="Premium *"
			android:textColor="@color/Black"
			android:textSize="14sp" />



		<TextView
			android:id="@+id/tv_premium_allocation_charge"
			style="@style/tv_heading"
			android:layout_width="0dp"
			android:layout_height="fill_parent"
			android:layout_weight="2"
			android:text="Premium Allocation Charge"
			android:textColor="@color/Black"
			android:textSize="14sp" />


		<TextView
			android:id="@+id/tv_policy_administration_charge"
			style="@style/tv_heading"
			android:layout_width="0dp"
			android:layout_height="fill_parent"
			android:layout_weight="2"
			android:text="Policy Administration Charge"
			android:textColor="@color/Black"
			android:textSize="14sp" />


		<TextView
			android:id="@+id/tv_service_tax"
			style="@style/tv_heading"
			android:layout_width="0dp"
			android:layout_height="fill_parent"
			android:layout_weight="2"
			android:text="Applicable Taxes"
			android:textColor="@color/Black"
			android:textSize="14sp" />


		<TextView
			android:id="@+id/tv_total_deduction"
			style="@style/tv_heading"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="2"
			android:text="Total deductions"
			android:textColor="@color/Black"
			android:textSize="14sp" />


		<TextView
			android:id="@+id/tv_opening_balance1"
			style="@style/tv_heading"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="1"
			android:text="Openining Balance1"
			android:textColor="@color/Black"
			android:textSize="14sp" />


		<TextView
			android:id="@+id/tv_mortality_charge1"
			style="@style/tv_heading"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="2"
			android:text="Mortality Charges"
			android:textColor="@color/Black"
			android:textSize="14sp" />


		<TextView
			android:id="@+id/tv_service_tax_on_mortality_charge1"
			style="@style/tv_heading"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="2"
			android:text="Applicable Taxes on Mortality Charges"
			android:textColor="@color/Black"
			android:textSize="14sp" />


		<TextView
			android:id="@+id/tv_guranteed_addition1"
			style="@style/tv_heading"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="2"
			android:text="Guarateed addition to policy account"
			android:textColor="@color/Black"
			android:textSize="14sp" />


		<TextView
			android:id="@+id/tv_extra_addition1"
			style="@style/tv_heading"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="2"
			android:text="Extra addition to policy account"
			android:textColor="@color/Black"
			android:textSize="14sp" />


		<TextView
			android:id="@+id/tv_fmc1"
			style="@style/tv_heading"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="1"
			android:text="FMC"
			android:textColor="@color/Black"
			android:textSize="14sp" />


		<TextView
			android:id="@+id/tv_service_tax_on_fmc1"
			style="@style/tv_heading"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="2"
			android:text="Applicable Taxes on FMC"
			android:textColor="@color/Black"
			android:textSize="14sp" />

		<TextView
			android:id="@+id/tv_closing_balance1"
			style="@style/tv_heading"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="2"
			android:text="Closing Balance"
			android:textColor="@color/Black"
			android:textSize="14sp" />

		<TextView
			android:id="@+id/tv_surrender_value1"
			style="@style/tv_heading"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="2"
			android:text="Surrender Value*"
			android:textColor="@color/Black"
			android:textSize="14sp" />


		<TextView
			android:id="@+id/tv_death_benefit1"
			style="@style/tv_heading"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="1"
			android:text="Death Benefit"
			android:textColor="@color/Black"
			android:textSize="14sp" />


		<TextView
			android:id="@+id/tv_opening_balance2"
			style="@style/tv_heading"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="1"
			android:text="Openining Balance2"
			android:textColor="@color/Black"
			android:textSize="14sp" />

		<TextView
			android:id="@+id/tv_mortality_charge2"
			style="@style/tv_heading"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="2"
			android:text="Mortality Charges"
			android:textColor="@color/Black"
			android:textSize="14sp" />


		<TextView
			android:id="@+id/tv_service_tax_on_mortality_charge2"
			style="@style/tv_heading"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="2"
			android:text="Applicable Taxes on Mortality Charges"
			android:textColor="@color/Black"
			android:textSize="14sp" />


		<TextView
			android:id="@+id/tv_guranteed_addition2"
			style="@style/tv_heading"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="2"
			android:text="Guarateed addition to policy account"
			android:textColor="@color/Black"
			android:textSize="14sp" />


		<TextView
			android:id="@+id/tv_extra_addition2"
			style="@style/tv_heading"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="2"
			android:text="Extra addition to policy account"
			android:textColor="@color/Black"
			android:textSize="14sp" />


		<TextView
			android:id="@+id/tv_fmc2"
			style="@style/tv_heading"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="1"
			android:text="FMC"
			android:textColor="@color/Black"
			android:textSize="14sp" />


		<TextView
			android:id="@+id/tv_service_tax_on_fmc2"
			style="@style/tv_heading"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="2"
			android:text="Applicable Taxes on FMC"
			android:textColor="@color/Black"
			android:textSize="14sp" />

		<TextView
			android:id="@+id/tv_closing_balance2"
			style="@style/tv_heading"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="2"
			android:text="Closing Balance"
			android:textColor="@color/Black"
			android:textSize="14sp" />

		<TextView
			android:id="@+id/tv_surrender_value2"
			style="@style/tv_heading"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="2"
			android:text="Surrender Value*"
			android:textColor="@color/Black"
			android:textSize="14sp" />


		<TextView
			android:id="@+id/tv_death_benefit2"
			style="@style/tv_heading"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="1"
			android:text="Death Benefit"
			android:textColor="@color/Black"
			android:textSize="14sp" />


		<TextView
			android:id="@+id/tv_commission"
			style="@style/tv_heading"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:layout_weight="2"
			android:text="Commission"
			android:textColor="@color/Black"
			android:textSize="14sp" />
	</TableRow>
</LinearLayout>
