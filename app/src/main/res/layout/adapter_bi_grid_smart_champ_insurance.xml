<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >

    <TableRow
        android:id="@+id/tr_header2"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_marginLeft="5dp"
        android:weightSum="11" >

        <TextView
            android:id="@+id/tv_adapter_bi_grid_smart_champ_insurance_end_year"
            style="@style/tv_heading"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1.1"
            android:text="End Year"
            android:textColor="@color/Black"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tv_adapter_bi_grid_smart_champ_insurance_total_base_premium"
            style="@style/tv_heading"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1.1"
            android:text="total base premium"
            android:textColor="@color/Black"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tv_adapter_bi_grid_smart_champ_insurance_death_benefit_guaranteed"
            style="@style/tv_heading"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1.1"
            android:text="guannteed death benefit"
            android:textColor="@color/Black"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tv_adapter_bi_grid_smart_champ_insurance_death_benefit_not_guaranteed_4_percentage"
            style="@style/tv_heading"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight=".95"
            android:text="non guaranteed death benefit 4 %"
            android:textColor="@color/Black"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tv_adapter_bi_grid_smart_champ_insurance_death_benefit_not_guaranteed_8_percentage"
            style="@style/tv_heading"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight=".95"
            android:text="non guaranteed death benefit 8 %"
            android:textColor="@color/Black"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tv_adapter_bi_grid_smart_champ_insurance_survival_benefit_guaranteed"
            style="@style/tv_heading"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1.1"
            android:text="guaranteed survival benefit"
            android:textColor="@color/Black"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tv_adapter_bi_grid_smart_champ_insurance_survival_benefit_not_guaranteed_4_percentage"
            style="@style/tv_heading"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:text="non guaranteed survival benefit 4 %"
            android:textColor="@color/Black"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tv_adapter_bi_grid_smart_champ_insurance_survival_benefit_not_guaranteed_8_percentage"
            style="@style/tv_heading"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:text="non guaranteed survival benefit 8 %"
            android:textColor="@color/Black"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tv_adapter_bi_grid_smart_champ_insurance_total_survival_benefit_4_percentage"
            style="@style/tv_heading"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1.5"
            android:text="total survival benefit 4%"
            android:textColor="@color/Black"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tv_adapter_bi_grid_smart_champ_insurance_total_survival_benefit_8_percentage"
            style="@style/tv_heading"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1.5"
            android:text="total survival benefit 8%"
            android:textColor="@color/Black"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tv_adapter_bi_grid_smart_champ_insurance_surrender_value_guaranteed"
            style="@style/tv_heading"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1.1"
            android:visibility="gone"
            android:text="guaranteed surrender value"
            android:textColor="@color/Black"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tv_adapter_bi_grid_smart_champ_insurance_surrender_value_not_guaranteed_4_percentage"
            style="@style/tv_heading"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:visibility="gone"
            android:text="non guranteed surrender value 4%"
            android:textColor="@color/Black"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tv_adapter_bi_grid_smart_champ_insurance_surrender_value_not_guaranteed_8_percentage"
            style="@style/tv_heading"
            android:layout_width="0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:visibility="gone"
            android:text="non guranteed surrender value 8%"
            android:textColor="@color/Black"
            android:textSize="14sp" />
    </TableRow>

</LinearLayout>